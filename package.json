{
  "name": "desafio",
  "version": "1.0.0",
  "description": "Desafio Node.js Axur",
  "main": "index.js",
  "author": "Guilherme Rocha Muzi Franco (guiPython)",
  "license": "MIT",
  "dependencies": {
    "@supercharge/collections": "^4.3.0",
    "axios": "^0.27.2",
    "config": "^3.3.7",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.4.0",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "rimraf": "^3.0.2",
    "tscpaths": "^0.0.9"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "up": "yarn build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --runInBand --silent",
    "test:coverage": "jest --runInBand --collect-coverage",
    "start": "yarn build && node dist/index.js"
  },
  "devDependencies": {
    "@types/config": "^0.0.41",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.38",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.0",
    "nock": "^13.2.4",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.2"
  }
}
